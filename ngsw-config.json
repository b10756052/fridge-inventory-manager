{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/fridge-inventory-manager/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/fridge-inventory-manager/favicon.ico",
          "/fridge-inventory-manager/index.html",
          "/fridge-inventory-manager/manifest.webmanifest",
          "/fridge-inventory-manager/*.css",
          "/fridge-inventory-manager/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/fridge-inventory-manager/assets/**",
          "/fridge-inventory-manager/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": [],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "10s",
        "strategy": "freshness"
      }
    }
  ]
}